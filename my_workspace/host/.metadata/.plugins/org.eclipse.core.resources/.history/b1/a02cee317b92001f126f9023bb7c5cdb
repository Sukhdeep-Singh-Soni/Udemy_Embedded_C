#include <stdio.h>

int main() {
	/*compiler takes 0x87 and 0xff00 as int which is 4 bytes, info loss*/
	unsigned char a = 0x87 + 0xff00; //0x00000087 | 0x0000ff00;
	float var = 100 / 8; //int / int = int (decimal truncated and info loss)
	printf("%d\n%f\n",a,var);
}



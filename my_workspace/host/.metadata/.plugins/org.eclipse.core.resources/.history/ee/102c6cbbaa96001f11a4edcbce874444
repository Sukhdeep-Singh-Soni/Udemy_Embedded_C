#include <stdio.h>

int main() {
	int age;
	printf("Enter age: ");
	scanf("%d", &age);
	if(age < 18) {
		printf("Sorry! you are not eligible to vote\n");
	} else {
		printf("Yes, you are eligible\n");
	}
//	if(age >= 18) {
//		printf("Yes, you are eligible\n");
//	}

	printf("Press enter to exit...");
	while(getchar() != '\n');
	getchar();
	return 0;
}

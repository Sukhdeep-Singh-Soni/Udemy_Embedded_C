#include <stdio.h>

int main() {
	int num1;
	printf("Enter a number for even or odd check: ");
	scanf("%d",&num1);
	int  bkp = num1;
	num1 = num1 | 0x90; //num1 | 0x10010000
		printf("[input] [output] %d %d\n", bkp, num1);
	return 0;
}

